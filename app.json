{
  "expo": {
    "name": "Workproof",
    "slug": "manage-time",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "backgroundColor": "#282828",
    "primaryColor": "#fff3e5",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#282828"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.workproof.app",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#282828"
      },
      "package": "com.managetime.app",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDGqyJR4KZRJt9qRLmeGjdlgIBt_nb7Kqw"
        }
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_LOCATION",
        "WAKE_LOCK"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permitir a ManageTime acceder a tu ubicaci√≥n."
        }
      ]
    ],
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "ts", "tsx", "json"],
      "assetExts": ["ttf", "png", "jpg", "jpeg", "mp3", "wav"]
    },
    "extra": {
      "excludeBackend": true
    }
  }
}
